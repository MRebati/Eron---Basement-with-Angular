{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "./base": ["./base/*"],
      "./base/interfaces": ["./base/interfaces/*"],
      "./authentication": ["./authentication/*"],
      "./base/services": ["./base/services/*"],
      "./base/pipes": ["./base/pipes/*"],
      "./base/directives": ["./base/directives/*"]
    },
    "baseUrl": "./",
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
